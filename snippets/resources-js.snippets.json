{
  "Freezer": {
		"scope": "javascript, typescript",
		"prefix": ["@freeze", "@objfreeze"],
		"body": [
			"Object.freeze(${1:objectName})",
			"$2"
		],
		"description": "Freezer Object Params"
	},
  "Promise": {
		"scope": "javascript, typescript",
		"prefix": "@promise",
		"body": [
			"new Promise((${1:resolve}) => {",
			"\t${2:Seu código aqui}",
			"});"
		],
		"description": "Create a new Promise"
	},
  "For": {
		"scope": "javascript, typescript",
		"prefix": "@for",
		"body": [
			"for(${2:var} ${1:i} = ${3:0}; ${1:i} < ${4:value}; ${1:i}++) {",
			"${5:\t//Digite seu código}",
			"}"
		],
		"description": "Loop Básico com o for"
	},
  "While": {
		"scope": "javascript, typescript",
		"prefix": "@while",
		"body": [
			"while(${1:condition}) {",
			"${2:\t//Digite seu código}",
			"}"
		],
		"description": "Loop Básico com o While"
	},
  "DoWhile": {
		"scope": "javascript, typescript",
		"prefix": "@dowhile",
		"body": [
			"do {",
			"${2:\t//Digite seu código}",
			"} while(${1:condition});"
		],
		"description": "Loop Básico com o for"
	},
  "Log": {
		"scope": "javascript, typescript",
		"prefix": "@console",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Saída do Log no console"
	},
  "Alert": {
		"scope": "javascript, typescript",
		"prefix": "@alert",
		"body": [
			"alert('$1');",
			"$2"
		],
		"description": "Alerta para o navegador"		
	},
  "Document Writer": {
		"prefix": "@docwrite",
		"body": [
			"document.write('$1');",
			"$2"
		],
		"description": "Escreve no documento HTML"		
	},
	"Function": {
		"prefix": "@function",
		"scope": "javascript, typescript",
		"body": [
			"function ${1:name}(${2:params}) {",
			"\t${3:#Digite aqui o seu código}",
			"}"
		],
		"description": "Defined one function"		
	},
	"Anonymous Function": {
		"prefix": "@anofunction",
		"scope": "javascript, typescript",
		"body": [
			"(${1:params}) => {",
			"\t${2:#Digite aqui o seu código}",
			"};"
		],
		"description": "Defined one function"		
	},
	"Define Property": {
		"prefix": "@defproperty",
		"scope": "javascript",
		"body": [
			"Object.defineProperty(${1:object}, ${2:newParam} , {",
			"\tenumerable: ${3:true}",
			"\twritable: ${4:true}",
			"\tvalue: ${5:true}",
			"});$6",
			""
		],
		"description": "Defined Property with especial functionalities."		
	},
	"Object Assign": {
		"prefix": "@defproperty",
		"scope": "javascript, typescript",
		"body": [
			"Object.assign(${1:destyne}, ${2:objects0...});",
			"$3"
		],
		"description": "Merge Objects for transform a new Object."		
	}
}